<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ http://wowprogramming.com/FrameXML/UI.xsd">
	<Button name="LiquidatorFrame" parent="UIParent" enableMouse="true" movable="true" frameStrata="LOW">
	<Size x="175" y="100" />
	<Anchors>
		<Anchor point="TOP" relativePoint="BOTTOM" relativeTo="Minimap">
			<Offset x="25" y="-25" />
		</Anchor>
	</Anchors>
	<Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
		<BackgroundInsets>
			<AbsInset left="11" right="12" top="12" bottom="11" />
		</BackgroundInsets>
		<TileSize>
			<AbsValue val="32" />
		</TileSize>
		<EdgeSize>
			<AbsValue val="32" />
		</EdgeSize>
	</Backdrop>
	<Layers>
		<Layer level="OVERLAY">
			<FontString name="$parentTitle" inherits="GameFontNormalSmall" justifyH="CENTER"  justifyV="TOP" setAllPoints="true" text="Liquidator - v0.1" >
			<Anchors>
				<Anchor point="TOP">
					<Offset x="0" y="-10" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentVendorBags" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPLEFT">
					<Offset x="10" y="-30" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentVendorBagsSale" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPRIGHT">
					<Offset x="-10" y="-30" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentVendorBank" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPLEFT">
					<Offset x="10" y="-40" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentVendorBankSale" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPRIGHT">
					<Offset x="-10" y="-40" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentVendorTxtTotal" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPLEFT">
					<Offset x="10" y="-50" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentVendorTotal" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPRIGHT">
					<Offset x="-10" y="-50" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentAuctionBags" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPLEFT">
					<Offset x="10" y="-60" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentAuctionBagsSale" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPRIGHT">
					<Offset x="-10" y="-60" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentAuctionBank" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPLEFT">
					<Offset x="10" y="-70" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentAuctionBankSale" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPRIGHT">
					<Offset x="-10" y="-70" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentAuctionTxtTotal" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPLEFT">
					<Offset x="10" y="-80" />
				</Anchor>
			</Anchors>
			</FontString>
			
			<FontString name="$parentAuctionTotal" inherits="GameFontNormalSmall" justifyH="LEFT" setAllPoints="true" >
			<Anchors>
				<Anchor point="TOPRIGHT">
					<Offset x="-10" y="-80" />
				</Anchor>
			</Anchors>
			</FontString>			
			
		</Layer>
	</Layers>
	<Scripts>
		<OnLoad>
			Liquidator_OnLoad(self)
		</OnLoad>
		<OnEvent>
			Liquidator_OnEvent(self, event, ...)
		</OnEvent>
		<OnDragStart>
			self:StartMoving()
		</OnDragStart>
		<OnDragStop>
			self:StopMovingOrSizing()
		</OnDragStop>
	</Scripts>
	</Button>
</Ui>
			